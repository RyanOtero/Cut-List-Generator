<Application x:Class="Solid_Price.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Views/MainWindow.xaml"
             Startup="App_Startup"
             Exit="App_Exit">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Themes/window-close-regular.xaml" />
                <ResourceDictionary Source="/Resources/Themes/window-minimize-solid.xaml" />
                <ResourceDictionary Source="/Resources/Themes/window-maximize-regular.xaml" />
                <ResourceDictionary Source="/Resources/Themes/TextStyle.xaml" />
                <ResourceDictionary Source="/Resources/Themes/ButtonStyle.xaml" />
                <ResourceDictionary Source="/Resources/Themes/RectBorderStyle.xaml" />
                <ResourceDictionary Source="/Resources/Themes/TabControlStyle.xaml" />
                <ResourceDictionary Source="/Resources/Themes/TitleFont.xaml" />
                <ResourceDictionary Source="/Resources/Themes/MainWindowStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>

